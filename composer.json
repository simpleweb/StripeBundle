{
    "name": "simpleweb/stripe-bundle",
    "type": "symfony-bundle",
    "description": "Symfony2 bundle that makes it easier to integrate the Stripe API library",
    "keywords": [ "simpleweb", "stripe" ],
    "license": "MIT",
    "authors": [
        {
            "name": "Steve Lacey",
            "email": "steve@simpleweb.co.uk"
        }
    ],
    "require": {
        "php": "~5.4",
        "doctrine/doctrine-bundle": "1.3.x-dev",
        "doctrine/orm": "~2.4",
        "stof/doctrine-extensions-bundle": "dev-master",
        "stripe/stripe-php": "~1.12"
    },
    "require-dev": {
        "phpunit/phpunit": "~3.7"
    },
    "autoload": {
        "psr-4": { "Simpleweb\\StripeBundle\\": "" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        }
    }
}
